{
  "manifest_version": 3,
  "name": "Simple Newtab",
  "author": "Cufoon",
  "version": "1.5.0",
  "description": "Just a clean new tab page.",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
